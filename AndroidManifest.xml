<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.taobao.taobao"
          android:installLocation="auto"
          android:versionCode="98"
          android:versionName="4.2.2" >

    <uses-sdk
            android:minSdkVersion="9"
            android:targetSdkVersion="18" />

    <application
            android:name="com.taobao.tao.TaobaoApplication"
            android:icon="@drawable/icon"
            android:label="@string/taobao_app_name" >
        <uses-library android:name="android.test.runner"/>
        <meta-data
                android:name="application"
                android:value="com.taobao.tao.TaoApplication" >
        </meta-data>

        <receiver android:name="com.taobao.tao.ReplacedReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.MY_PACKAGE_REPLACED" />
            </intent-filter>
        </receiver>
        
        <activity android:name="android.taobao.atlas.runtime.NutActivity" >
        </activity>

        <service android:name="com.taobao.nut.NutService1" >
        </service>
        <service android:name="com.taobao.nut.NutService2" >
        </service>
        <service android:name="com.taobao.nut.NutService3" >
        </service>
        <service android:name="com.taobao.nut.NutService4" >
        </service>
        <service android:name="com.taobao.nut.NutService5" >
        </service>

    </application>

</manifest>
