<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.taobao.taobao"
          android:installLocation="auto"
          android:versionCode="98"
          android:versionName="4.2.2.0.1" >

    <uses-sdk
            android:minSdkVersion="9"
            android:targetSdkVersion="18" />

    <application
            android:name="com.taobao.tao.TaobaoApplication"
            android:icon="@drawable/icon"
            android:label="@string/taobao_app_name" >

        <meta-data
                android:name="application"
                android:value="com.taobao.tao.TaoApplication,com.taobao.browser.BrowserApplication" >
        </meta-data>

        <receiver android:name="com.taobao.tao.ReplacedReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.MY_PACKAGE_REPLACED" />
            </intent-filter>
        </receiver>

    </application>

</manifest>
